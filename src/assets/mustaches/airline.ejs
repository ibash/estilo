<% if(it.ctrlp1){ %>
if !get(g:, 'loaded_ctrlp', 0)
  finish
endif

let s:CP1 = [ "<%= it.ctrlp1.fg.hex %>", "<%= it.ctrlp1.bg.hex %>", <%= it.ctrlp1.fg.xterm %>, <%= it.ctrlp1.bg.xterm %> ]
