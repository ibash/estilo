let g:airline#themes#<%= it.info.name %>#palette.insert = airline#themes#generate_color_map(s:insert1, s:insert2, s:insert3)

let s:replace1 = [ "<%= it.replace1.fg.hex %>", "<%= it.replace1.bg.hex %>", <%= it.replace1.fg.xterm %>, <%= it.replace1.bg.xterm %> ]
