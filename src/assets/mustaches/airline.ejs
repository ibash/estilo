<% const info = it.info; %>
""
" Airline_theme: <%= info.name %>

<% if(info.description){ %>" Description: <%= info.description %>
<% } %>

<% if(info.url){%>" URL: <%= info.url %>
<% } %>

<% if(info.author){%>" Author: <%= info.author %>
<% } %>

<% if(info.license){%>" License: <%= info.license %>
<% } %>

""

let g:airline#themes#<%= info.name %>#palette = {}

let s:normal1 = [ "<%= it.normal1.fg.hex %>", "<%= it.normal1.bg.hex %>", <%= it.normal1.fg.xterm %>, <%= it.normal1.bg.xterm %> ]
let s:normal2 = [ "<%= it.normal2.fg.hex %>", "<%= it.normal2.bg.hex %>", <%= it.normal2.fg.xterm %>, <%= it.normal2.bg.xterm %> ]
let s:normal3 = [ "<%= it.normal3.fg.hex %>", "<%= it.normal3.bg.hex %>", <%= it.normal3.fg.xterm %>, <%= it.normal3.bg.xterm %> ]
let g:airline#themes#<%= it.info.name %>#palette.normal = airline#themes#generate_color_map(s:normal1, s:normal2, s:normal3)

let s:insert1 = [ "<%= it.insert1.fg.hex %>", "<%= it.insert1.bg.hex %>", <%= it.insert1.fg.xterm %>, <%= it.insert1.bg.xterm %> ]
let s:insert2 = [ "<%= it.insert2.fg.hex %>", "<%= it.insert2.bg.hex %>", <%= it.insert2.fg.xterm %>, <%= it.insert2.bg.xterm %> ]
