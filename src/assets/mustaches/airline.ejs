let g:airline#themes#<%= it.info.name %>#palette.replace = airline#themes#generate_color_map(s:replace1, s:replace2, s:replace3)

let s:visual1 = [ "<%= it.visual1.fg.hex %>", "<%= it.visual1.bg.hex %>", <%= it.visual1.fg.xterm %>, <%= it.visual1.bg.xterm %> ]
