let g:airline#themes#<%= it.info.name %>#palette.visual = airline#themes#generate_color_map(s:visual1, s:visual2, s:visual3)

let s:inactive1 = [ "<%= it.inactive1.fg.hex %>", "<%= it.inactive1.bg.hex %>", <%= it.inactive1.fg.xterm %>, <%= it.inactive1.bg.xterm %> ]
