{
  "name": "@jacoborus/estilo",
  "version": "2.0.0-beta.9",
  "exports": "./estilo.ts",
  "tasks": {
    "bundle-assets": "deno run --allow-read --allow-write src/bundle-assets.ts",
    "install": "deno task bundle-assets  && deno install --allow-read --allow-write --allow-net --allow-env estilo.ts"
  },
  "test": {
    "files": {
      "include": [
        "test/"
      ]
    }
  },
  "imports": {
    "eta/": "npm:eta@v1.14.2",
    "@std/fs": "jsr:@std/fs@0.219.0",
    "@std/path": "jsr:@std/path@0.219.0",
    "@std/yaml": "jsr:@std/yaml@0.219.0",
    "cliffy/": "https://deno.land/x/cliffy@v1.0.0-rc.3/",
    "hexterm/": "npm:hexterm@2.0.2/",
    "@std/testing": "jsr:@std/testing@0.163.0"
  }
}
